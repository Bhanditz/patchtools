// Test basic patch
// Patching BasicClass

// . == detect
.class ~Basic

    .method ~sayHello ()Ljava/lang/String;
        .any
        // - == remove
        -ldc "Hello bob"
        // + == add
        +ldc "Hello jim"
        .areturn
        .any
    .end-method

    +method addedMethod ()Ljava/lang/String;
        +ldc "Hello world"
        +areturn
    +end-method

    .method ~staticCreate ()L~Basic; static
        .any
        -ldc "Testing"
        +ldc "Cake"
        .any
        .areturn
        .any
    .end-method
.end-class