// Test basic patch
// Patching BasicClass

// . == detect

public class ~Basic {

    public java.lang.String ~sayHello() {
        // - == remove
        -push-string "Hello bob"
        // + == add
        +push-string "Hello jim"
        .return
    }

    add public java.lang.String addedMethod() {
        +push-string "Hello world"
        +return
    }

    public static ~Basic ~staticCreate() {
        .any
        -push-string "Testing"
        +push-string "Cake"
        .any
        .return
    }
}
